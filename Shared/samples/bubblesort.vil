fn print_table(t) {
  if (typeof(t) != "table") {
    io.println("Attempt to print out something that's not a table.");
    exit;
  }
  
  each(loc k, v : t) {
    io.print(@v + " ");
  }
  
  io.println();
}

loc t = { 3, 9, 6, 9, 1, 2, 3, 100, 20, 33 };

print_table(t);

for (loc i = 0; i < #t - 1; i++) {
  for (loc j = 0; j < #t - i - 1; j++) {
    if (t[j] > t[j + 1]) {
      loc tmp = t[j];
      t[j] = t[j + 1];
      t[j + 1] = tmp;
    }
  }
}

print_table(t);
