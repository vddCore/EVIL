#[test] fn eq() -> 2 == 2;
#[test] fn ne() -> 10 != 5;
#[test] fn gt() -> 10 > 2;
#[test] fn lt() -> 1 < 3;
#[test] fn gte() -> 2 >= 2;
#[test] fn lte() -> 4 <= 4;

#[test(4)] fn add_int() -> 2 + 2;
#[test(21.37)] fn add_fp64() -> 21 + 0.37;

#[test(2)] fn sub_int() -> 3 - 1;
#[test(2.137)] fn sub_fp64() -> 3 - 0.863;

#[test(4)] fn div_int() -> 20 / 5;
#[test(213.7)] fn div_fp64() -> 1923.3 / 9;

#[test(4)] fn mod_int() -> 9 % 5;
#[test(2.5); approximate] fn mod_fp64() -> 21 % 3.7;

#[test(80)] fn mul_int() -> 4 * 20;
#[test(0.2137)] fn mul_fp64() -> 20 * 0.010685;

#[test(0x0F00)] fn band() -> 0xFFFF & 0x0F00;
#[test(0x000F)] fn bxor() -> 0xFFFF ^ 0xFFF0;
#[test(0xFFFF)] fn bnot() -> ~0 & 0xFFFF;
#[test(0xFFF0)] fn bor() -> 0x00F0 | 0xF000 | 0x0F00;

#[test(16)] fn shl() -> 1 << 4;
#[test(32)] fn shr() -> 0x80 >> 2;

#[test] fn exist_str() -> "a" in "blah";
#[test] fn noexist_str() -> !("f" in "blah");
#[test] fn exist_tbl() -> "a" in { "a" => 2137 };
#[test] fn noexist_tbl() -> !("a" in { });

#[test(7)] fn len_str() -> #"ąćężółń";
#[test(4)] fn len_tbl() -> #{2,1,3,7};

#[test("4")] fn tostr_num() -> @4;
#[test("asdfg")] fn tostr_str() -> @"asdfg";
#[test("true")] fn tostr_bool_true() -> @true;
#[test("false")] fn tostr_bool_false() -> @false;

#[test(2137)] fn tonum_int() -> $"2137";
#[test(21.37)] fn tonum_fp64() -> $"21.37";
#[test(0)] fn tonum_bool_false() -> $(false);
#[test(1)] fn tonum_bool_true() -> $(true);

#[test]
fn postinc_var() {
  rw var a = 10;
  ret (a++ == 10) && (a == 11);
}

#[test]
fn postinc_tbl_member() {
  var a = { 10 };
  ret (a[0]++ == 10) && (a[0] == 11);
}

#[test]
fn preinc_var() { 
  rw var a = 10;
  ret (++a == 11) && (a == 11);
}

#[test]
fn preinc_tbl_member() {
  var a = { 10 };
  ret (++a[0] == 11) && (a[0] == 11);
}

#[test]
fn postdec_var() {
  rw var a = 10;
  ret (a-- == 10) && (a == 9);
}

#[test]
fn postdec_tbl_member() {
  var a = { 10 };
  ret (a[0]-- == 10) && (a[0] == 9);
}

#[test]
fn predec_var() {
  rw var a = 10;
  ret (--a == 9) && (a == 9);
}

#[test]
fn predec_tbl_member() {
  var a = { 10 };
  ret (--a[0] == 9) && (a[0] == 9);
}