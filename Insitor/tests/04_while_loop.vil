#[test(10)]
fn while_simple() {
  var a = 0;
  
  while (a != 10) {
    a++;
  }
  
  ret a;
}

#[test(5)]
fn while_break() {
  var a = 0;
  
  while (a != 10) {
    a++;
    
    if (a == 5) {
      break;
    }
  }
  
  ret a;
}

#[test(2)]
fn while_skip() {
