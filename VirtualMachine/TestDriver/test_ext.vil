fn add(a, b) {
  ret a + b;
}

fn sub(a, b) {
  ret a - b;
}

fn div(a, b) {
  ret a / b;
}

fn export() {
    ret {
        "add" => add,
        "sub" => sub,
        "div" => div,
        
        "mul" => fn(a, b) {
            ret a * b;
        },
        
        "pow" => fn(a) {
            ret a * a;
        }
    };
}